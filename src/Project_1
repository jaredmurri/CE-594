import numpy as np
import scipy
import math


density= 1153

kin_viscos= 0.008


def diameter (x):
    if x < 1:
        return 1
    elif x>4.5:
        return 4
    else:
        return (4/4.5)*(x-1)


def velocity(diameter):
    return 92/(diameter**2)

def Reynolds_number (x):
    return (velocity(x)*diameter(x))/kin_viscos

